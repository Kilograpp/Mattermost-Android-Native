<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="oval">
            <gradient
                android:angle="90"
                android:endColor="@color/icon_team_color_start"
                android:startColor="@color/icon_team_color_shadow"
                android:type="linear" />
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:right="0.5dp" android:left="0.5dp" android:bottom="0.5dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <gradient
                android:angle="90"
                android:endColor="@color/icon_team_color_start"
                android:startColor="@color/icon_team_color_end"
                android:type="linear" />
            <size
                android:width="@dimen/circle_image_size"
                android:height="@dimen/circle_image_size" />
        </shape>
    </item>
</layer-list>
